<!DOCTYPE html>
<html lang="zh-CN">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>vue-spa-demo-with-wp4</title>
  <!-- css-inject -->
  <% for (let url of manifest.css) { %>
    <link href="<%= url %>" rel="stylesheet">
  <% } %>
</head>
<body>
  <script>window.initialData = JSON.parse('<%- JSON.stringify(initialData) %>');</script>
  <div id="app"></div>
  <div class="variables">
    process.env variables:
    <%= process.env.NODE_ENV %>
    <br>
    initialData:
    <%- JSON.stringify(initialData) %>
    <br>
    manifest:
    <%- JSON.stringify(manifest) %>
  </div>
  <!-- js-inject -->
  <% for (let url of manifest.js) { %>
    <script type="text/javascript" src="<%= url %>" ></script>
  <% } %>
</body>
</html>